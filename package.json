{
  "name": "talk-core",
  "version": "0.0.1-alpha",
  "description": "State Management Library - based on ngrx (Redux).",
  "main": "./bundles/talk-core.umd.js",
  "module": "./talk-core.es5.js",
  "es2015": "./talk-core.js",
  "typings": "./talk-core.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build:watch": "gulp build:watch",
    "dev:watch": "gulp build:watch",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build-demo",
    "start": "concurrently \"npm run build-demo:watch\" \"npm run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "npm run build-test",
    "test": "concurrently \"npm run build-test:watch\" \"karma start karma.conf.js\"",
    "coverage": "karma start karma-coverage.conf.js --single-run",
    "pretest:once": "npm run build-test",
    "test:once": "karma start karma.conf.js --single-run",
    "preintegration": "npm run build && cd integration && npm run clean && npm install",
    "integration": "npm run integration:aot && npm run integration:jit",
    "integration:jit": "cd integration && npm run e2e",
    "integration:aot": "cd integration && npm run e2e:aot",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "release": "standard-version"
  },
  "peerDependencies": {
    "@angular/animations": ">=4.0.0 <5.0.0",
    "@angular/common": ">=4.0.0 <5.0.0",
    "@angular/core": ">=4.0.0 <5.0.0",
    "@angular/flex-layout": ">=2.0.0-beta.9 <3.0.0",
    "@angular/forms": ">=4.0.0 <5.0.0",
    "@angular/platform-browser": ">=4.0.0 <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0 <5.0.0",
    "@angular/router": ">=4.0.0 <5.0.0",
    "@ngrx/core": ">=1.2.0 <2.0.0",
    "@ngrx/effects": ">=4.0.0 <5.0.0",
    "@ngrx/store": ">=4.0.0 <5.0.0",
    "@ngx-translate/core": ">=8.0.0 <9.0.0",
    "@ngx-translate/http-loader": ">=2.0.0 <3.0.0",
    "ngrx-store-localstorage": ">=0.2.4 <1.0.0",
    "ngrx-store-logger": ">=0.2.0 <1.0.0"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^4.1.0",
    "@ngrx/router-store": "^4.1.0",
    "@ngrx/store": "^4.1.0",
    "@ngrx/store-devtools": "^4.0.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "camelcase": "^4.0.0",
    "concurrently": "3.4.0",
    "core-js": "^2.5.1",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "iconfonts": "^0.12.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-coverage": "^0.1.4",
    "lite-server": "^2.2.2",
    "ngrx-store-localstorage": "^0.2.4",
    "ngrx-store-logger": "^0.2.0",
    "node-sass": "^4.5.3",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.2",
    "standard-version": "^4.2.0",
    "systemjs": "^0.20.19",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.18"
  }
}
